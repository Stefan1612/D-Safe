{
  "name": "D-Tweet",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=12.3.0"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/lab": "^5.0.0-alpha.71",
    "@mui/material": "^5.4.4",
    "@netlify/functions": "^1.0.0",
    "@openzeppelin/contracts": "^4.5.0",
    "@snapshot-labs/snapshot.js": "^0.3.54",
    "chrome-aws-lambda": "^10.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "ethers": "^5.5.4",
    "form-data": "^4.0.0",
    "ipfs-http-client": "^57.0.3",
    "node-fetch": "^2.6.7",
    "puppeteer-core": "^13.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.1",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "5.0.0",
    "serverless-http": "^2.7.0",
    "vanta": "^0.5.21"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-etherscan": "^2.1.8",
    "@nomiclabs/hardhat-waffle": "^2.0.2",
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ethereum-waffle": "^3.4.0",
    "hardhat": "^2.8.3",
    "hardhat-gas-reporter": "^1.0.7",
    "prettier": "^2.5.1",
    "prettier-plugin-solidity": "^1.0.0-beta.19",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.18",
    "source-map-explorer": "^2.5.2"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "dev": "react-scripts start",
    "start": "cross-env NODE_ENV=development netlify dev --port 5000",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint '**' --no-error-on-unmatched-pattern && npm run solhint",
    "lint:fix": "eslint '**' --no-error-on-unmatched-pattern --fix && npm run solhint:fix",
    "solhint": "solhint contracts/*.sol",
    "solhint:fix": "solhint contracts/*.sol --fix",
    "format": "prettier '**' -c --no-error-on-unmatched-pattern --ignore-unknown",
    "format:fix": "prettier '**' -w --no-error-on-unmatched-pattern --ignore-unknown"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "supports es6-module"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "Twitter",
    "Tweet",
    "NFT",
    "Crypto",
    "Opensea",
    "MetaMask",
    "Metadata",
    "Mint",
    "Ethereum",
    "ERC-721",
    "IPFS"
  ]
}
